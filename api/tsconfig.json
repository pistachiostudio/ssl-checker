{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es5",
			"es6",
			"es7",
		],
		"rootDir": "./src",
		"module": "esnext",
		"downlevelIteration": true,
		"strict": true,
		"noUnusedLocals": true,
		"esModuleInterop": true,
		"moduleResolution": "node"
	},
	"include": [
		"./src/**/*"
	]
}